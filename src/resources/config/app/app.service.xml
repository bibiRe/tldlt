			</bean>
		</property>
	</bean>

	<bean id="deviceService" parent="baseTransactionProxyApp">
		<property name="target">
			<bean class="com.sysware.tldlt.app.service.device.DeviceServiceImpl">
				<property name="g4Dao" ref="g4Dao" />

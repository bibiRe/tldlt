		<property name="target">
			<bean class="com.sysware.tldlt.app.service.devmgr.DevMgrServiceImpl">
				<property name="g4Dao" ref="g4Dao" />
				<property name="appDao" ref="appDao" />
			</bean>
		</property>
	</bean>

	<bean id="InspectionPlanService" parent="baseTransactionProxyApp">

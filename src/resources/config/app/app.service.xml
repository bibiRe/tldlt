	</bean>


	<bean id="UserGroupService" parent="baseTransactionProxyApp">
		<property name="target">
			<bean class="com.sysware.tldlt.app.service.usergroup.UserGroupServiceImpl">
				<property name="g4Dao" ref="g4Dao" />

				<property name="g4Dao" ref="g4Dao" />
				<property name="appDao" ref="appDao" />
			</bean>
		</property>
	</bean>
	
	<bean id="mediaUrlService"
		class="com.sysware.tldlt.app.service.media.MediaUrlServiceGlobalParamImpl">
		<property name="g4Dao" ref="g4Dao" />
	</bean>
	<bean id="mediaPathService"
		class="com.sysware.tldlt.app.service.media.MediaPathServiceGlobalParamImpl">
		<property name="g4Dao" ref="g4Dao" />
	</bean>

	<bean id="deviceTypeCheckItemService"
		class="com.sysware.tldlt.app.service.devicetypecheckitem.DeviceTypeCheckItemServiceImpl">
		<property name="g4Dao" ref="g4Dao" />
		<property name="appDao" ref="appDao" />
	</bean>
	
	<bean id="deviceFaultService"
		class="com.sysware.tldlt.app.service.devicefault.DeviceFaultServiceImpl">
		<property name="g4Dao" ref="g4Dao" />
		<property name="appDao" ref="appDao" />
	</bean>

</beans>
